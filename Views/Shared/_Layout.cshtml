    <!-- Mobile Metas -->
    <meta name="viewport" clienttemplate="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <!-- Site Metas -->
    <meta name="keywords" clienttemplate="" />
    <meta name="description" clienttemplate="" />
    <meta name="author" clienttemplate="" />

    <title>@ViewBag.Title</title>
    

    <!-- bootstrap core css -->
    <link href="~/clienttemplate/css/bootstrap.css" rel="stylesheet" />
    <!-- fonts style -->
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap" rel="stylesheet">

    <!--owl slider stylesheet -->
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />

    <!-- font awesome style -->
    <link href="~/clienttemplate/css/font-awesome.min.css" rel="stylesheet" />
    <!-- nice select -->
